{
  "name": "tourze/doctrine-function-bundle",
  "description": "Doctrine函数增强模块",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "beberlei/doctrineextensions": "^1.5.0",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/doctrine-function-collection": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineFunctionBundle\\": "src"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}